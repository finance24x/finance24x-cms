{
  "kind": "collectionType",
  "collectionName": "cities",
  "info": {
    "singularName": "city",
    "pluralName": "cities",
    "displayName": "City",
    "description": "Specific locations where rates might vary"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::state.state",
      "inversedBy": "cities"
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "cities"
    },
    "dailyRates": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::daily-rate.daily-rate",
      "mappedBy": "city"
    }
  }
}

